package factorial.com;
import java.util.Scanner;
public class Factorial {

	public static void main(String[] args) {
		
				int i, fact=1;
				int number=10;
				for(i=1; i<=number; i++)
				    {
					    fact=fact*i;

			        }
			System.out.println("Factorial of number 10 is:"+fact);
			

	}

 }
